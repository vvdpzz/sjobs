<h1>Location List</h1>

<table>
  <tr>
    <th>User_id</th>
    <th>User_name</th>
    <th>longitude</th>
    <th>latitude</th>
    <th>time</th>
  </tr>

<% @location.each do |location| %>
  <tr>
    <td><%= location.user_id %></td>
    <td><%= location.user_name %></td>
    <td><%= location.longitude %></td>
    <td><%= location.latitude %></td>
    <td><%= location.time %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Location', new_location_path %>